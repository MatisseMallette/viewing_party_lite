<div class='centered_text'>
  <%= button_to 'Discover Page', discover_user_path(@user), method: :get %>
</div>

  <table id='results' class='centered_table'>
    <% @movies.each_with_index do |movie, index| %>
      <tr id="movie-<%= index %>" class='title_col'>
        <td><%= link_to movie.title, user_movie_path(@user, movie.id, type: 'show') %></td>

        <td class='vote_col'>Vote Average: <%= movie.vote_average %></td>
      </tr>
    <% end %>
  </table>
